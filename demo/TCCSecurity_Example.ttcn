  var BIT_1 direction := '1'B
  var BIT_1_65504 msg_in := oct2bit('AD9C441F890B38C457A49D421407E8'O)
  var BIT_1_65504 msg_out:= f_calculate128_EEA1(key,count,bearer,direction,msg_in);
